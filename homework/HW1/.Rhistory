knitr::opts_chunk$set(echo = TRUE)
set.seed(293875)
nanoparticles <- data.frame(size_nm = round(rnorm(50,mean=30,sd=7),2),
zeta_potential = round(rnorm(50,-35,5),2),
poly_index = round(exp(rnorm(50,log(0.27),0.8)),3),
spherical = round(runif(50,0,1)),
quality = sample(c(1:5),50,prob=c(0.1,0.1,0.2,0.5,0.1),replace=TRUE))
nanoparticles
write.table(nanoparticles, file="~/Dropbox/TXCL7565/homework/HW1/nanoparticles.csv",sep=",",col.names=TRUE,row.names=FALSE,quote=FALSE)
